export fun abs(x: Integer): Integer {
    if (x < 0) {
        return 0 - x
    } else {
        return x
    }
}

export fun min(a: Integer, b: Integer): Integer {
    if (a < b) {
        return a
    } else {
        return b
    }
}

export fun max(a: Integer, b: Integer): Integer {
    if (a > b) {
        return a
    } else {
        return b
    }
}