import { print } from "bolt:stdio"

val numbers := [10, 20, 30, 40, 50]

print("Array iteration test:")

var index := 0
var sum := 0

for (index < 5) {
    val value := numbers[index]
    sum = sum + value
    print("Element at ")
    print(index)
    print(": ")
    print(value)
    index = index + 1
}

print("Total sum: ")
print(sum)